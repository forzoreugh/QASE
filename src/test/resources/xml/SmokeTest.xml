<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="2" name="QASE Smoke Test" parallel="tests">
    <listeners>
        <listener class-name="tests.ui.LoginTest"/>
    </listeners>
    <groups>
        <run>
            <include name="smoke"/>
        </run>
    </groups>
    <test name="Тестовый набор смоук-тестов" preserve-order="true">
        <classes>
            <class name="tests.ui.LoginTest">
                <methods>
                    <include name="checkValideLogin"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>
